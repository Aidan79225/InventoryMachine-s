<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aidan.secondinventoryworkplatform"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.camera" />
    <application
        android:name="com.aidan.secondinventoryworkplatform.Application"
        tools:replace="android:label"
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/check_system"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.aidan.secondinventoryworkplatform.StartActivity"
            android:windowSoftInputMode="adjustPan">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aidan.secondinventoryworkplatform.FragmentManager.FragmentManagerActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>
    </application>


</manifest>